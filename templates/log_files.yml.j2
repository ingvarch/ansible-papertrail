# {{ ansible_managed }}
files:
{% for log in papertrail.logs %}
  - {{ log.path }}
{% endfor %}
destination:
  host: {{ papertrail.host }}
  port: {{ papertrail.port }}
  protocol: tls
hostname: {{ papertrail.hostname }}  # override OS hostname
new_file_check_interval: "10" # Check every 10 seconds
